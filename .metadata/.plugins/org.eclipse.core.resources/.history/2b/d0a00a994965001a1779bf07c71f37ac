package qwerty;

public class first {

	public static void main(String[] args) {
		
			    int row=14;
			    for (int i=0; i<row; i++){
			        for (int w=1; w<8-i; w++ ){
			        System.out.print(w);
			        for (int r=1; i<14; r++) {
			        	System.out.print(r);
			        }
			        }System.out.println();
			        
			      }
			  


	}

}
