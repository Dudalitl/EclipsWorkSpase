package qwerty;

public class first {

	public static void main(String[] args) {
		
		
			
				int[][] a = {
					{1,1,2}, //sum = 4
					{3,1,2}, //sum = 6
					{3,5,3}, //sum = 11
					{0,1,2}  //sum = 3
				};
				int sum1=0,sum2=0,sum3=0,sum4=0;
				for (int i=0; i<a.length; j++){
				  for (int j=0; j<4; i++){
				    sum1=sum1+a[0][j];
				    sum2=sum2+a[1][j];
				    sum3=sum3+a[2][j];
				    sum4=sum4+a[3][j];
				  }
				}System.out.println(sum1);
				
				
				
				
			
			}
		}