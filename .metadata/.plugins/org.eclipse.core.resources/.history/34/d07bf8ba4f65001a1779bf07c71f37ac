package qwerty;

public class first {

	public static void main(String[] args) {
		
			    int row=14;
			    for (int i=0; i<row; i++){
			    	if (i<7) {
			        for (int w=1; w<8-i; w++ ){
			        System.out.print(w);
			        }System.out.println();
			      }
			    	else {
			    	for (int w=1; w<=i-5; w++ ){
				        System.out.print(w);
				        }System.out.println();
			    }
			 
			  


	}
	}
}
