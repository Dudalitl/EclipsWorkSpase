package qwerty;

public class first {

	public static void main(String[] args) {

		int[][] a = { { 1, 1, 2 }, // sum = 4
				{ 3, 1, 2 }, // sum = 6
				{ 3, 5, 3 }, // sum = 11
				{ 0, 1, 2 } // sum = 3
		};
		int[] b= new int[3];

		for (int i = 0; i < 3; i++) {
			for (int j = 0; j < a[i].length; j++) {
				b[i]=b[i]+a[i][j];
				

			}System.out.println(b[i] );
		}
	}
}