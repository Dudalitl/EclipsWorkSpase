package HW;

public class EncapsulationDemo {

		  private String empName;
		  private int empAge;
		  public String gettere(){
		    return empName;
		  }
		  public void setter(String empName) {
		  this.empName=empName;
		}
		  public int getter(){
		    return empAge;
		  }
		  public void setter(int empAge){
		    this.empAge=empAge;
		  }
}
