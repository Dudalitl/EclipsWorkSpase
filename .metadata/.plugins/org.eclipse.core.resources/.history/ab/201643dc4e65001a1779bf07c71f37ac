package qwerty;

public class first {

	public static void main(String[] args) {
		
			    int row=14;
			    for (int i=0; i<row; i++){
			        for (int w=1; w<8-i; w++ ){
			        System.out.print(w);
			        }System.out.println();
			      
			    	for (int w=1; w<row-w; w++ ){
				        System.out.print(w);
				        }System.out.println();
			    }
			 
			  


	}
	}
